pipeline {
    agent any

    stages {
        stage('Built') {
            steps {
                echo 'Building..'
                sh 'mvn package'
            }
        }
        stage('Tested') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deployed') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
